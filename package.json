    "scripts": {
        "start": "set NODE_ENV=production&node --require @babel/register ./bin/www ",
        "devstart": "set NODE_ENV=development&set DEBUG=dev&nodemon --exec babel-node ./bin/www",
        "lint": "./node_modules/.bin/eslint ./",
